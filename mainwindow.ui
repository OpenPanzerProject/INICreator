                 <string notr="true">QTextEdit {
	background: #454F65;
	color: #FFF;
}

/* Scroll area */
QScrollBar:vertical {
     border: 0; /*1px solid #808080; */
     background: #E1E3E3;	/* light grey */
     width: 17px;
     margin: 20px 0 20px 0;
 }
/* Scroll handle */
QScrollBar::handle:vertical {
	border: 1px solid grey;
     background: #454F65; /* Dark purple grey */
     min-height: 20px;
 }

/* The up-arrow area */
QScrollBar::sub-line:vertical {
     border: 1px solid grey;
     background: #454F65;
     height: 16px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }

/* The down-arrow area */
QScrollBar::add-line:vertical {
     border: 1px solid grey;
     background: #454F65;
     height: 16px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }

QScrollBar::up-arrow:vertical {
	image: url(:/images/consoleuparrow.png);
 }

/* Yes, we swap arrows when pressed. It looks cool. */
/* And for some reason we weren't able to change the background color, */
/* which is all we really wanted to do. */
QScrollBar::up-arrow:vertical:pressed {
 	image: url(:/images/consoledownarrow.png);
 }

QScrollBar::down-arrow:vertical {
	image: url(:/images/consoledownarrow.png);
}

QScrollBar::down-arrow:vertical:pressed {
	image: url(:/images/consoleuparrow.png);
}

/* No idea what this is */
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: none;
 }</string>
