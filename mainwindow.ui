/* FORM BACKGROUND
/* ----------------------------------------------------------------------------------------------------------------------------*/
/* This is the main background color of the overall form */
QMainWindow#MainWindow, QWidget#centralWidget {
	background: #454F65;	/* Dark purple grey */
}


/* ----------------------------------------------------------------------------------------------------------------------------*/
/* MENU BAR
/* ----------------------------------------------------------------------------------------------------------------------------*/
/* Menu bar */
QMenuBar{
    padding-left: 8px;
	background: #454F65;	/* Dark purple grey */
	color: #FFF;
}
QMenuBar::item {
/*    spacing: 3px;  spacing between menu bar items */
    padding: 10px 4px 2px 6px;
	background: #454F65;
}
/* when selected using mouse or keyboard, OR when hovering (there is no hover) */
QMenuBar::item:selected { 
    background: #39085A;	/* Purple */
}
QMenuBar::item:pressed {
	border-style: inset;	/*sunken*/
}
/* Menu Items */
QMenu {
    background-color: white;
    margin: 2px; /* some spacing around the menu */
}
QMenu::item {
    padding: 2px 25px 2px 20px;
    border: 1px solid transparent; /* reserve space for selection border */
}
QMenu::item:selected {
    border-color: #39085A;
	background: #E1E3E3;	/* Light purple grey */
}
QMenu::separator {
    height: 1px;
    background: #454F65;
/*    margin-left: 10px;
    margin-right: 5px;*/
}


/* ----------------------------------------------------------------------------------------------------------------------------*/
/* OPEN PANZER HEADER FRAME
/* ----------------------------------------------------------------------------------------------------------------------------*/
QFrame#frmHeader{
	background: #39085A;	/* Purple */
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
QPushButton#cmdHideHeader{
	image: url(:/images/minimize.png);
	border: none;
	background: #39085A;
	padding: 1px;
	min-height: 11px;
}
QPushButton#cmdHideHeader:hover {
	image: url(:/images/minimize_hover.png);
}


/* ----------------------------------------------------------------------------------------------------------------------------*/
/* SUB-HEADER: DEVICE NAME, AND SERIAL SETTINGS
/* ----------------------------------------------------------------------------------------------------------------------------*/
QFrame#frmSerial{
	border: 0;
    border-bottom: 1px solid #666;
/*    border-top: 1px solid #000; */
	background-color: #FFF;	/* white */
}
QPushButton#cmdRefreshCOMs{
    image: url(:/images/refresh.png);
}


/* ----------------------------------------------------------------------------------------------------------------------------*/
/* STACKED WIDGET
/* ----------------------------------------------------------------------------------------------------------------------------*/
/* This is our widget with buttons along the left side and a content area on the right */
/* Background of area where all the main content boxes are */
QStackedWidget#stackedWidgetMain{ 
	border-left: 0;
	background: #E1E3E3; 	/* Light purple grey */
}


/* ----------------------------------------------------------------------------------------------------------------------------*/
/* LISTVIEW - THESE ARE THE BUTTONS ALONG THE LEFT SIDE
/* ----------------------------------------------------------------------------------------------------------------------------*/
QListView {
    show-decoration-selected: 1; /* make the selection span the entire width of the view */
  /*   background: #E1E3E3;	/* Light purple grey */
    background: #39085A;	/* Purple */
	color: #000;	/* font-color black */
}
/* This is the item not-selected and not-hovered */
QListView::item {
	background: #39085A;	/* Purple */
	color: #FFF;	/* font-color white */
	padding: 10; 
	border-top: 1px solid #FFF; /* White */
	border-bottom: 1px solid #FFF; /* White */
/*	height: 56px;  -- this seems to make no difference. Instead use Grid Size Height in QListView properties */
	margin: 0;
}
/* Selected */
QListView::item:selected, QListView::item:selected:active { 
	/* Horizontal gradient from white on the left to light purple grey on the right, blending into the light purple grey of the form */
	background: qlineargradient(x1:1, y1:0, x2: 0, y2: 0, stop: 0 #E1E3E3, stop: 1 #FFFFFF);
	color: #000;
	border-right: 0;
}
/* Hover */
QListView::item:!selected:hover {
	background: #454F65;	/* Dark purple grey */
	color: #FFF;
}


/* ----------------------------------------------------------------------------------------------------------------------------*/
/* BUTTONS
/* ----------------------------------------------------------------------------------------------------------------------------*/
QPushButton{
    border: 1px solid #666;
	background: #E4E4F1;
	padding: 1px;
	min-height: 10px;
/*    border-radius: 3px;  */
}
QPushButton:pressed, QPushButton:checked {
    background-color: #C1CADE;
	padding-top: 2px;
}
QPushButton:flat {
    border: none; /* no border for a flat push button */
}
QPushButton:hover {
	border: 1px solid #000;
}

/* ----------------------------------------------------------------------------------------------------------------------------*/
/* COMBO BOXES 																														  
/* ----------------------------------------------------------------------------------------------------------------------------*/
QComboBox {
    border: 1px solid #666;
    padding: 1px 10px 1px 3px;
/*    border-radius: 3px; */
}
/* This is if the value is editable, which mine are probably not going to be */
QComboBox:editable {
    background: white;
}
/* Background color at rest */
QComboBox:!editable, QComboBox::drop-down:editable 
{
	background: white;
}
/* QComboBox gets the &quot;on&quot; state when the list is expanded*/
/* Note that drop-down refers to the down-arrow box. We are styling it here as well in the case where the list is open. */
QComboBox:!editable:on, QComboBox::drop-down:editable:on, QComboBox::drop-down:!editable:on
{
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #E4E4F1, stop: 1 #F7F3F7); 
/*	background: #E4E4F1; */
}
/* shift the text when the list expands */
QComboBox:on 
{ 
    padding-top: 3px;
}
 /* shift the arrow when the list expands */
QComboBox::down-arrow:on 
{
    top: 1px;
}
/* The &quot;drop-down&quot; is the little box that holds the down arrow - not the drop-down list */
/* This is the basic format at rest, or when expanded if not over-written by the specific settings for expanded (above) */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
	background: #E4E4F1;
    border-left-width: 1px;
    border-left-color: #666;
    border-left-style: solid; /* just a single line */
/*    border-top-right-radius: 3px; 
    border-bottom-right-radius: 3px; */
}
QComboBox::down-arrow:enabled {
	image:url(:/images/downarrow.png);
}
QComboBox::down-arrow:disabled {
	image:url(:/images/downarrowdisabled.png);
}
QComboBox:disabled {
	background: #E1E3E3;
}
/* The box that is shown when expanded is not part of the actual combo box, it is of the QAbstractItemView type */
QComboBox QAbstractItemView {
    border: 2px solid #666;;
	/* This means the selection that you've highlighted while the list is dropped-down */
    selection-background-color: #C1CADE;
	color: #000000;
	/* This is the background color of the dropped-down list */
	background: white;
}

