/* ----------------------------------------------------------------------------------------------------------------------------*/
/* SPIN and DOUBLE SPIN BOXES
/* ----------------------------------------------------------------------------------------------------------------------------*/
QSpinBox, QDoubleSpinBox { 
    border: 1px solid #666;
    padding-right: 16px;
/*    border-radius: 3px; */
}

QSpinBox:disabled, QDoubleSpinBox:disabled { 
	background: #E1E3E3;
}
QSpinBox::up-button, QDoubleSpinBox::up-button
{
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 15px; /* 15 + 2*1px border-width = 16px padding + 1 px parent border */
	background-color: #E4E4F1;
	border-width: 1px;
    border: 1px solid #666;
/*    border-top-right-radius: 3px; */
}	

QSpinBox::down-button, QDoubleSpinBox::down-button
{
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    width: 15px; /* 15 + 2*1px border-width = 16px padding + 1 px parent border */
	background-color: #E4E4F1;
    border: 1px solid #666;
	border-width: 1px;
	border-top: 0px;
/*    border-bottom-right-radius: 3px;  */
}	
QSpinBox::up-arrow, QDoubleSpinBox::up-arrow {
	image: url(:/images/spin_uparrow.png);
/*    width: 7px;
    height: 7px; */
}
QSpinBox::down-arrow, QDoubleSpinBox::down-arrow {
	image: url(:/images/spin_downarrow.png);
}
QSpinBox::up-arrow:disabled, QDoubleSpinBox::up-arrow:disabled {
	image: url(:/images/spin_uparrow_disabled.png);
}
QSpinBox::down-arrow:disabled, QDoubleSpinBox::down-arrow:disabled {
	image: url(:/images/spin_downarrow_disabled.png);
}


/* ----------------------------------------------------------------------------------------------------------------------------*/
/* CHECK BOXES
/* ----------------------------------------------------------------------------------------------------------------------------*/
QCheckBox {
    spacing: 5px;
}
QCheckBox::indicator {
    width: 14px;
    height: 14px;
}
QCheckBox::indicator:checked {
    image: url(:/images/checkbox_checked.png);
}
QCheckBox::indicator:unchecked {
    image: url(:/images/checkbox_unchecked.png);
}
QCheckBox::indicator:checked:hover {
    image: url(:/images/checkbox_checked_hover.png);
}
QCheckBox::indicator:unchecked:hover {
    image: url(:/images/checkbox_unchecked_hover.png);
}
QCheckBox::indicator:disabled {
    image: url(:/images/checkbox_disabled_unchecked.png);	
}
QCheckBox::indicator:checked:disabled {
    image: url(:/images/checkbox_disabled_checked.png);	
}


